@{

}
<el-button @@click="btnClick">点击我</el-button>
<el-button @@click="test">测试</el-button>
<select id="sex">
    <option value="">请选择</option>
    <option value="1">男</option>
    <option value="0">女</option>
</select>
<tree-table :data="tableData" name="name" id-name="id" parent-name="pid">
    <el-table-column prop="date"
                     label="日期"
                     width="180">
    </el-table-column>
    <el-table-column prop="name"
                     label="姓名"
                     width="180">
    </el-table-column>
    <el-table-column prop="address"
                     label="地址">
    </el-table-column>
</tree-table>


@section styles{
    <style>
        select#sex {
            width: 100px;
        }

        select option[value=''] {
            color: gray;
        }
    </style>
}
